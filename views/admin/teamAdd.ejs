<h1>Team add</h1> <div>
    <div class="col-xl-6 col-lg-6">
        <div class="card">
            <div class="card-header">
                <h4 class="card-title">Jamoa qo'shish</h4>
            </div>

            <div class="card-body">
                <div class="basic-form">
                    <form action="/team/create" method="POST" enctype="multipart/form-data">
                        <div class="mb-3">
                            <label class="form-label">Kursni tanlang</label>
                            <select name="course_Id" class="default-select  form-control wide">
                                <% for (let item of course) { %>
                                <option value="<%= item._id %>"><%= item.name %></option>
                                <%}%>
                            </select>
                        </div>
                        <br>
                        <div class="mt-4 mb-3">
                            <input 
                                type="text"  
                                class="form-control input-default " 
                                placeholder="Ism"
                                name="name"
                                required="true"
                            >
                        </div>

                        <div class="mb-3">
                            <input 
                                type="text" 
                                class="form-control input-default " 
                                placeholder="Kasbi"
                                name="job" 
                                required="true"
                            >
                        </div>

                        <div class="mb-3">
                            <input 
                                type="number" 
                                class="form-control input-default " 
                                placeholder="Telefon raqam"
                                name="phone"
                                required="true"
                            >
                        </div>

                        <div class="mb-3">
                            <input 
                                type="file" 
                                class="form-control input-default"
                                name="image"
                                required="true"
                            >
                        </div>
                        <div class="mb-3">
                            <input 
                                type="text" 
                                class="form-control input-default " 
                                placeholder="Instagram"
                                name="instagram" 
                                required="true"
                            >
                        </div>
                        <div class="mb-3">
                            <input 
                                type="text" 
                                class="form-control input-default " 
                                placeholder="Facebook"
                                name="facebook" 
                                required="true"
                            >
                        </div>
                        <div class="mb-3">
                            <input 
                                type="text" 
                                class="form-control input-default " 
                                placeholder="Telegram"
                                name="telegram" 
                                required="true"
                            >
                        </div>
                        <div class="mb-3">
                            <input 
                                type="text" 
                                class="form-control input-default " 
                                placeholder="Website"
                                name="website" 
                                required="true"
                            >
                        </div>
                        <div class="mb-3">
                            <textarea 
                            placeholder="Jamoa azosi haqida yaxshi yozing hamma o'qiydi"
                            class="form-control" 
                            cols="10"
                            rows="10" 
                            id="comment"
                            name="description"
                            ></textarea>
                        </div>
                       <button class="btn btn-primary" type="submit">Yuklash</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div> 